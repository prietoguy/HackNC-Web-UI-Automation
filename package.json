{
  "name": "hack-nc-ui-automation-demo",
  "version": "1.0.0",
  "description": "A demo for the Intro to Web UI Automation workshop at HackNC.",
  "main": "index.js",
  "scripts": {
    "webdriver": "node node_modules/protractor/bin/webdriver-manager update; node node_modules/protractor/bin/webdriver-manager start",
