  "license": "ISC",
  "dependencies": {
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "cucumber": "~1.3.1",
    "protractor": "~4.0.10",
    "protractor-cucumber-framework": "^0.6.0"
  },
  "devDependencies": {
    "webdriver-manager": "^10.2.6"
  }
}
